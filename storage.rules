rules_version = '2';

// See: https://firebase.google.com/docs/storage/security
service firebase.storage {
  match /b/{bucket}/o {
    // A an example, this rule allows any authenticated user to read and write all files
    // in your Storage bucket. To learn more, read the documentation linked above.
    match /{allPaths=**} {
      allow read, write: if request.auth != null;
    }
  }
}
