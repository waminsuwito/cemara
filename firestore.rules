rules_version = '2';

service cloud.firestore {
  match /databases/{database}/documents {
    match /{document=**} {
      // PERINGATAN: Aturan ini mengizinkan siapa saja untuk membaca dan menulis.
      // Ini berguna untuk pengembangan tetapi tidak aman untuk produksi.
      allow read, write: if true;
    }
  }
}
